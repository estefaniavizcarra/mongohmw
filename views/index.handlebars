
    <div class="container">

        <div class="row">
            <div class="col-md-12">

                {{#each article}}
                <div class="card">
                    <div class="card-body">
                        <img class="activator img" src="{{img}}">

                        <h5 class="card-title">{{this.title}}</h5>
                        <div class="card-image waves-effect waves-block waves-light">

                        <a href="https://www.refinery29.com/{{this.link}}"" class="card-link">
                            <i class="far fa-newspaper"></i> Read Article on Refinery 29</a>
                        <br>
                        <br>
                        <p class="card-text">Article Summary: {{this.summary}}</p>

                        <br>
                        <button type="button" class="btn btn-success save" data-id={{this._id}}>Save Article</button>

                    </div>
                </div>
                {{/each}}

            </div>
        </div>
    </div>