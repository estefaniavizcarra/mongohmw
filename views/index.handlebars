<body>



   
    <div class="container">

        <div class="row">
            <div class="col-md-12">

                {{#each article}}
                <div class="card">
                 
                    <div class="card-body">
                          <div class="card-image img">
                              <img src="{{img}}"> 
                              </div>

                        <h5 class="card-title">{{this.title}}</h5>
                        <div class="card-image waves-effect waves-block waves-light">
                 
                        <p class="card-text">Article Summary: {{this.summary}}</p>

                        <div class="valign-wrapper">
                   <a  class ="btn btn-success liink valign-wrapper" href="https://www.refinery29.com/{{this.link}}"" class="card-link">
                            Read Article on Refinery 29</a>

                        <a class="btn btn-success save valign-wrapper" data-id={{this._id}}>Save Article</a>
                    </div>
                </div>
                            </div>
                            </div>

                {{/each}}

        </div>
    </div>
 
 </body>